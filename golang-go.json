{
  "regexManagers": [
    {
      "fileMatch": ["{{arg0}}"],
      "matchStrings": [
        " +['\"]?version['\"]? *: +['\"]?(?<currentValue>[^'\" \\n]+)['\"]? +# renovate: depName=golang/go[ \\n]",
        " +['\"]?name['\"]? *: +['\"]?golang/go@(?<currentValue>[^'\" \\n]+)['\"]?"
      ],
      "extractVersionTemplate": "^go(?<version>.*)$",
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "golang/go"
    }
  ]
}
